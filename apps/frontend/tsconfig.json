{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "shared-types": ["../../packages/shared-types"]
    },
    "outDir": "dist",
    "rootDir": "../.."
  },
  "include": ["src", "../../packages/shared-types"],
  "exclude": ["node_modules", "dist"]
}
